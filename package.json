{
  "name": "crowdstrike",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "inferno": "^3.10.1",
    "inferno-component": "^3.10.1",
    "inferno-scripts": "5.0.0",
    "node-sass-chokidar": "^0.0.3",
    "prop-types": "^15.6.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
